Attribute VB_Name = "vbwErrHandler"
Option Explicit

Public Enum vbwEnumRetCode
    vbwEnd = vbAbort           ' = 3 '
    vbwRetry = vbRetry         ' = 4 '
    vbwIgnoreLine = vbIgnore   ' = 5 '
    vbwAlwaysIgnore
    vbwDebug ' not used here
    vbwDoDumpVariable
    vbwCollapse ' not used here
End Enum
Public vbwRetCode As vbwEnumRetCode

Global vbwMessageString As String
Global vbwCircumstancesString As String
Global vbwErrorPath As String ' the directory where we will store files to zip and email
Global vbwfHasReported As Boolean

Private Declare Function ShellExecute Lib "shell32.dll" Alias "ShellExecuteA" (ByVal hwnd As Long, ByVal lpOperation As String, ByVal lpFile As String, ByVal lpParameters As String, ByVal lpDirectory As String, ByVal nShowCmd As Long) As Long
Private Const SW_SHOWNORMAL = 1

Dim vbwTurnOffTimersString As String
Private Declare Function MessageBox Lib "user32" Alias "MessageBoxA" (ByVal hwnd As Long, ByVal lpText As String, ByVal lpCaption As String, ByVal wType As Long) As Long
Private Const vbMsgBoxSetTopMost = &H40000

Dim vbwTypeInfo As String
Dim vbwExcludedProperties As String

Private Declare Function GetForegroundWindow Lib "user32" () As Long
Private Declare Function SetForegroundWindow Lib "user32" (ByVal hwnd As Long) As Long
Private Declare Function GetDesktopWindow Lib "user32" () As Long
Dim hWndForeground As Long

Dim StrRememberErrors As String
Dim StrLastError As String

' vbwNoTraceProc vbwNoTraceLine ' don't remove this !
' vbwNoErrorHandler ' don't remove this !


Public Function vbwErrorHandler( _
        ByVal ErrNumber As Long, _
        ByVal ErrDescription As String, _
        ByVal ErrProject As String, _
        ByVal ErrSource As String, _
        ByVal ErrProcedure As String, _
        ByVal ErrLine As Long) As vbwEnumRetCode

    vbwRetCode = 0

    If ErrNumber <> -1 Then ' ErrNumber = -1 would indicate that we're just back from dumping

        ' check if the user doesn't want to see this error anymore '
        StrLastError = Chr(0) & ErrSource & Chr(1) & ErrNumber & Chr(1) & ErrLine & Chr(0)
        If InStr(StrRememberErrors, StrLastError) Then
           vbwRetCode = vbwIgnoreLine
           Exit Function
        End If

        ' save current Err object for later reporting
        vbwSaveErrObject

        ' don't let timer events interfere with the error handling process
        TurnOffTimers

        ' store which window was foreground in case we want to dump a screenshot of it later '
        hWndForeground = GetForegroundWindow()


        vbwMessageString = "An unexpected error has occurred in this application: " & vbCrLf & vbCrLf & _
                       "Error " & ErrNumber & vbCrLf & _
                       "Description: " & ErrDescription & vbCrLf & _
                       "Project: " & ErrProject & vbCrLf & _
                       "Module: " & ErrSource & vbCrLf & _
                       "Procedure: " & ErrProcedure & vbCrLf & _
                       "Line: " & ErrLine & vbCrLf & _
                       "Version: " & App.Major & "." & App.Minor & "." & App.Revision & vbCrLf

        vbwfHasReported = False
        vbwFrmErrHandler.Show 1

        If vbwRetCode = vbwDoDumpVariable Then

            ' init variable dump
            On Error Resume Next
            ' prepare directory where we will store files to zip
            vbwErrorPath = Replace(App.Path & "\", ":\\", ":\") & "Error Report Files\"
            Kill vbwErrorPath & "*.*"
            RmDir vbwErrorPath
            MkDir vbwErrorPath
            On Error GoTo 0

            vbwDumpFile = vbwErrorPath & "Variables.txt"

            ' instanciate a vbwFunctionsVB6.AdvancedFunctions object for future use
            On Error Resume Next
            Set vbwAdvancedFunctions = CreateObject("vbwFunctionsVB6.AdvancedFunctions")
            fIsVbwFunctionsInitialized = (Err.Number = 0)

            If fIsVbwFunctionsInitialized Then
                ' set this to False if you don't wish to display the status to the user
                vbwAdvancedFunctions.ShowStatus = True
                ' Init the VB Watch Functions Library object                                 '
                ' Please refer to the help file under the VB Watch Protector Reference topic '
                vbwAdvancedFunctions.DumpFileName = vbwDumpFile
                vbwAdvancedFunctions.DumpMaxStringLength = vbwDumpStringMaxLength
                vbwAdvancedFunctions.DumpMaxObjectLength = 1000000
                ' The following is internal data used to dump objects found in your project - Don't modify !'
            vbwExcludedProperties = ""

                vbwAdvancedFunctions.ExcludedProperties = vbwExcludedProperties ' VB Watch will replace <VBW EXCLUDED PROPERTIES> with the correct value of vbwExcludedProperties
            vbwTypeInfo = ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""
            vbwTypeInfo = vbwTypeInfo & ""

                vbwAdvancedFunctions.ObjectInfo = vbwTypeInfo ' VB Watch will replace <VBW OBJECT FILE INFO> with the correct value of vbwTypeInfo
            End If
        End If

    Else
        ' Continuing from Sub vbwFrmErrHandler.cmdSendMail_click '

        vbwfHasReported = True

        ' Now the variable dump file is in vbwErrorPath & "Variables.txt" '
        ' Let's include some other useful infos                           '

        Dim ff As Long
        On Error Resume Next
        If vbwTraceProc Then
            ' write stack and trace data
            If vbwCallStack Then
                ff = FreeFile
                Open vbwErrorPath & "Stack.txt" For Output As #ff
                Print #ff, vbwGetStack()
                Close #ff
            End If

            If vbwLogTraceToFile Then
                If FileExist(vbwLogFile) Then
                  Open vbwErrorPath & "Trace.txt" For Output As #ff
                  Print #ff, LoadFile(vbwLogFile)
                  Close #ff
                End If
            End If
        End If

        ' write error data
        ff = FreeFile
        Open vbwErrorPath & "Error.txt" For Output As #ff
            Print #ff, vbwMessageString & vbCrLf & vbCrLf & "Circumstances:" & vbwCircumstancesString
        Close #ff

        Dim success As Long
        Dim msg As String, I As Long
        If fIsVbwFunctionsInitialized Then
            If vbwSystemInfo Then
               ' write file version data '
               vbwAdvancedFunctions.ExeInstance = App.hInstance
               vbwAdvancedFunctions.ExeCommandLine = Command$
               vbwAdvancedFunctions.VBRuntimeFile = "VBA6.DLL"     ' VB Watch will replace with the correct value E.g. "MSVBVM60.DLL"     '
               vbwAdvancedFunctions.ComponentsUsed = "StdFormat;DAO;MSBind;MSDataReportLib;DERuntimeObjects;COMSVCSLib;Excel;TabDlg;MSDataGridLib;MSAdodcLib;MSDataListLib;MSChart20Lib;MSComDlg;MSFlexGridLib;MSHierarchicalFlexGridLib;RichTextLib;MSComCtl2;MSDATAREPORTRUNTIMELIB;MSSTDFMT;RICHTEXT;DERUNTIME"   ' VB Watch will replace with the correct value E.g. "MSGrid;MSComCtl2" '
               Open vbwErrorPath & "Version.txt" For Output As #ff
               Print #ff, vbwAdvancedFunctions.GetVersionInfo
               Close #ff

               ' write user's system data '
               Open vbwErrorPath & "System.txt" For Output As #ff
               Print #ff, vbwAdvancedFunctions.GetSystemInfo
               Close #ff
            End If
            If vbwScreenshot Then
               ' captures screenshot of the active window                                                                             '
               ' (you may also use vbwAdvancedFunctions.GetWindowScreenShot(GetDesktopWindow) for a screenshot of a the full desktop) '
               If hWndForeground Then
                    SetForegroundWindow hWndForeground
               End If
               SavePicture vbwAdvancedFunctions.GetWindowScreenShot(hWndForeground), vbwErrorPath & "Screenshot.bmp"
            End If

            Dim title As String
            title = "Error report for " & ErrProject & " " & App.Major & "." & App.Minor & "." & App.Revision

            ' zip files to send '
            If vbwAdvancedFunctions.ZipDirectory(vbwErrorPath, title & ".zip") = 0 Then
                ' successful                                                          '
                ' this will create a button allowing the user to open the report file '
                vbwFrmErrHandler.Report = vbwErrorPath & title & ".zip"
            End If

            ' email error report '
            success = vbwAdvancedFunctions.SendMessage(vbwEmailRecipientAdress, title, _
                       vbwMessageString & vbCrLf & vbCrLf & "Circumstances:" & vbwCircumstancesString _
                           & vbCrLf & vbCrLf & "(Note to sender: the attached file is " & vbwErrorPath & title & ".zip - Size: " _
                           & VBA.FileLen(vbwErrorPath & title & ".zip") \ 1024 & " Kb)", _
                       vbwErrorPath & title & ".zip")

            If success <> 0 Then
                ' the SendMessage method failed (see error codes in the help file)            '
                ' so we have to try an alternative method:                                    '
                ' command line: mailto:<address>?subject=<Message Subject>&body=<Message Body>'
                ' Warning ! command lines of more than 256 characters may be truncated !      '

                success = ShellExecute(0&, "open", _
                               "mailto:" & vbwEmailRecipientAdress & _
                               "?subject=" & title & _
                               "&body=" & "(Please attach: " & vbwErrorPath & title & ".zip - Size: " & VBA.FileLen(vbwErrorPath & title & ".zip") \ 1024 & " Kb to this message.)", _
                               "", "c:\", SW_SHOWNORMAL)
                If success > 32 Then
                    ' was successful to execute the previous command      '
                    ' but sometimes this isn't enough to send the message '
                    If MessageBox(0&, "An email message should be displayed on your computer now, ready for sending." & vbCrLf & "Have you been able to attach the required file and send the message successfully ?", App.title, vbYesNo + vbQuestion + vbMsgBoxSetTopMost) = vbNo Then   '
                        success = 32  ' simulates fail          '
                    End If
                End If

                If success <= 32 Then
                    ' failed again !                         '
                    ' let the user send the message manually '
                    msg = "Impossible to send the error report automatically !" & vbCrLf & vbCrLf
                    msg = msg & "Please open your email messenger and send a message with the following parameters:" & vbCrLf
                    msg = msg & "Address: " & vbwEmailRecipientAdress & vbCrLf
                    msg = msg & "Subject: " & title & vbCrLf
                    msg = msg & "Message: Please open the attached file." & vbCrLf
                    msg = msg & "Attached file: " & vbwErrorPath & title & ".zip (Size: " & VBA.FileLen(vbwErrorPath & title & ".zip") \ 1024 & " Kb)" & vbCrLf & vbCrLf
                    msg = msg & "Would you like to copy this message to the clipboard ?" & vbCrLf
                    If MessageBox(0&, msg, App.title, vbYesNo + vbQuestion + vbMsgBoxSetTopMost) = vbYes Then
                       For I = 1 To 10
                          Clipboard.Clear
                          Clipboard.SetText msg, vbCFText
                       Next I
                    End If
                End If
            End If
        Else
            msg = "Error:" & vbCrLf & LoadFile(vbwErrorPath & "Error.txt") & vbCrLf & _
                         "Stack:" & vbCrLf & LoadFile(vbwErrorPath & "Stack.txt") & vbCrLf & _
                         "Variables:" & vbCrLf & LoadFile(vbwErrorPath & "Variables.txt")
            For I = 1 To 10
                Clipboard.Clear
                Clipboard.SetText msg, vbCFText
            Next I
            success = ShellExecute(0&, "open", _
                           "mailto:" & vbwEmailRecipientAdress & _
                           "?subject=" & title & _
                           "&body=" & "Please paste the contents of the clipboard below (Press CTRL+V):", _
                           "", "c:\", SW_SHOWNORMAL)

            If success > 32 Then
                ' was successful to execute the previous command      '
                ' but sometimes this isn't enough to send the message '
                If MessageBox(0&, "Have you been able to send the message successfully ?", App.title, vbYesNo + vbQuestion + vbMsgBoxSetTopMost) = vbNo Then   '
                    success = 32  ' simulates fail          '
                End If
            End If

            If success <= 32 Then
                  ' failed again !                         '
                  ' let the user send the message manually '
                  msg = "Please open your email messenger and send a message with the following parameters:" & vbCrLf & _
                               "Address: " & vbwEmailRecipientAdress & vbCrLf & _
                               "Subject: " & title & vbCrLf & _
                               "Message: " & vbCrLf & _
                               msg
                  For I = 1 To 10
                      Clipboard.Clear
                      Clipboard.SetText msg, vbCFText
                  Next I
                  MessageBox 0&, "Impossible to send the error report automatically !" & vbCrLf & vbCrLf & _
                                        "Please send the message manually." & vbCrLf & _
                                        "Full instructions have been copied to the clipboard." & vbCrLf & _
                                        "Thank you.", App.title, 0&
            End If
        End If

        ' success - remove temp files '
        On Error Resume Next
        Kill vbwErrorPath & "Screenshot.bmp"
        Kill vbwErrorPath & "System.txt"
        Kill vbwErrorPath & "Version.txt"
        Kill vbwErrorPath & "Error.txt"
        Kill vbwErrorPath & "Trace.txt"
        Kill vbwErrorPath & "Stack.txt"
        Kill vbwErrorPath & "Variables.txt"
        On Error GoTo 0

        Set vbwAdvancedFunctions = Nothing

        vbwFrmErrHandler.Show 1
    End If

    If vbwRetCode = vbwAlwaysIgnore Then
        ' remember this error to ignore it next time '
        StrRememberErrors = StrRememberErrors & StrLastError
        vbwRetCode = vbwIgnoreLine
    End If

    vbwErrorHandler = vbwRetCode

    If vbwRetCode <> vbwDoDumpVariable And vbwRetCode <> vbwEnd Then
        TurnOnTimers
    End If
  
End Function

' gets the whole file in a string
Private Function LoadFile(ByVal sFileName As String) As String
On Error GoTo erreur
    Dim nFile As Integer, sText As String
    nFile = FreeFile
    'Open sFileName For Input As nFile ' Don't do this!!!
    If Not FileExist(sFileName) Then
         Exit Function
    End If
    ' Let others read but not write
    Open sFileName For Binary As nFile
    sText = String$(LOF(nFile), 0)
    Get nFile, 1, sText
    Close nFile
    LoadFile = sText
erreur:
End Function

' function FileExist not requiring a dir$ call
Private Function FileExist(ByVal tFile As String) As Boolean
    On Error Resume Next
    Call VBA.FileLen(tFile)
    FileExist = (Err = 0)
End Function

' This disables all active timers because they might raise events during error handling time, '
' which would lead to unexpected results.                                                     '
' If you have other "outside" sources of events in your project, such as custom timers or     '
' subclassing code, this is the place to disable them temporarily.                            '
Private Sub TurnOffTimers()
    Dim f As Form
    Dim c As Control

    On Error Resume Next
    vbwTurnOffTimersString = Chr(0) ' Chr(0) = delimiter
    For Each f In Forms
        For Each c In f.Controls
            If TypeName(c) = "Timer" Then
                If c.Enabled = True Then
                    ' remember its state by way of its object address
                    vbwTurnOffTimersString = vbwTurnOffTimersString & ObjPtr(c) & Chr(0)
                    c.Enabled = False
                End If
            End If
        Next c
    Next f
End Sub

Private Sub TurnOnTimers()
    Dim f As Form
    Dim c As Control

    On Error Resume Next
    For Each f In Forms
        For Each c In f.Controls
            If TypeName(c) = "Timer" Then
                If InStr(vbwTurnOffTimersString, Chr(0) & ObjPtr(c) & Chr(0)) > 0 Then
                    If ObjPtr(c) > 0 Then
                        c.Enabled = True
                    End If
                End If
            End If
        Next c
    Next f
End Sub


